{
  "spec": {
    "name": "codercm",
    "services": [
      {
        "name": "code-server",
        "image": {
          "registry_type": "DOCKER_HUB",
          "repository": "linuxserver/code-server"
        },
        "envs": [
          {
            "key": "PUID",
            "value": "1000"
          },
          {
            "key": "PGID",
            "value": "1000"
          },
          {
            "key": "TZ",
            "value": "Europe/London"
          },
          {
            "key": "PASSWORD",
            "value": ""
          },
          {
            "key": "HASHED_PASSWORD",
            "value": ""
          },
          {
            "key": "SUDO_PASSWORD",
            "value": ""
          },
          {
            "key": "SUDO_PASSWORD_HASH",
            "value": ""
          },
          {
            "key": "DEFAULT_WORKSPACE",
            "value": "/config/workspace"
          }
        ],
        "http_port": 8443
      }
    ]
  }
}
